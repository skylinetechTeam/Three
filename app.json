{
  "expo": {
    "name": "Three",
    "slug": "Three",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#1737e8",
      "resizeMode": "contain",
      "image": "./assets/logo.png"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#1737e8",
        "resizeMode": "contain",
        "image": "./assets/logo.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1737e8"
      },
      "splash": {
        "backgroundColor": "#1737e8",
        "resizeMode": "contain",
        "image": "./assets/logo.png",
        "imageWidth": 200,
        "imageHeight": 200
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir $(PRODUCT_NAME) que use a sua localização."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5b82dbdf-d906-4f6e-91d0-3f7e3fb80d8f"
      }
    }
  }
}
