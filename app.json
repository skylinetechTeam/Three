{
  "expo": {
    "name": "Travel",
    "slug": "Travel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/travel.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#1737e8",
      "resizeMode": "contain",
      "image": {
        "_": "Removendo a imagem e configurando para usar texto"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.igorsemedo.travel",
      "adaptiveIcon": {
        "foregroundImage": "./assets/travel.png",
        "backgroundColor": "#1737e8"
      }
    },
    "web": {
      "favicon": "./assets/travel.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir $(PRODUCT_NAME) que use a sua localização."
        }
      ]
    ]
  }
}
